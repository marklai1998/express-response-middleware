{
  "name": "express-response-middleware",
  "version": "2.2.0",
  "description": "Transform an express response",
  "files": [
    "dist"
  ],
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.mts",
  "exports": {
    ".": {
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      }
    }
  },
  "typesVersions": {
    "*": {
      "*": [
        "./dist/index.d.mts"
      ]
    }
  },
  "scripts": {
    "build": "tsdown --format esm --format cjs",
    "test": "vitest run --coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/marklai1998/express-response-middleware"
  },
  "keywords": [
    "mung",
    "middleware",
    "transform",
    "response",
    "express"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/marklai1998/express-response-middleware/issues"
  },
  "homepage": "https://github.com/marklai1998/express-response-middleware#readme",
  "devDependencies": {
    "@biomejs/biome": "2.4.4",
    "@commitlint/cli": "20.4.2",
    "@marklai/shared-config-biome": "^1.0.0",
    "@marklai/shared-config-commitlint": "1.0.0",
    "@marklai/shared-config-typescript": "^1.1.0",
    "@types/express": "5.0.6",
    "@types/node": "24.10.13",
    "@types/supertest": "6.0.3",
    "@vitest/coverage-v8": "4.0.18",
    "lefthook": "2.1.1",
    "supertest": "7.2.2",
    "tsdown": "0.20.3",
    "typescript": "5.9.3",
    "vitest": "4.0.18"
  },
  "peerDependencies": {
    "express": "^4.0.0 || ^5.0.0"
  },
  "packageManager": "pnpm@10.30.1",
  "engines": {
    "node": ">=18"
  }
}
