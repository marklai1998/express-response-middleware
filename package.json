{
  "name": "express-response-middleware",
  "version": "2.1.3",
  "description": "Transform an express response",
  "files": [
    "dist"
  ],
  "main": "dist/cjs/index.cjs",
  "module": "dist/esm/index.mjs",
  "types": "dist/esm/index.d.ts",
  "exports": {
    ".": {
      "require": {
        "types": "./dist/cjs/index.d.cts",
        "default": "./dist/cjs/index.cjs"
      },
      "import": {
        "types": "./dist/esm/index.d.ts",
        "default": "./dist/esm/index.mjs"
      }
    }
  },
  "typesVersions": {
    "*": {
      "*": [
        "./dist/esm/index.d.ts"
      ]
    }
  },
  "scripts": {
    "build": "rollup -c",
    "test": "vitest run --coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/marklai1998/express-response-middleware"
  },
  "keywords": [
    "mung",
    "middleware",
    "transform",
    "response",
    "express"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/marklai1998/express-response-middleware/issues"
  },
  "homepage": "https://github.com/marklai1998/express-response-middleware#readme",
  "devDependencies": {
    "@biomejs/biome": "2.2.5",
    "@commitlint/cli": "20.1.0",
    "@commitlint/config-conventional": "20.0.0",
    "@types/express": "5.0.3",
    "@types/node": "22.18.10",
    "@types/supertest": "6.0.3",
    "@vitest/coverage-v8": "3.2.4",
    "lefthook": "1.13.6",
    "rollup": "4.52.4",
    "rollup-plugin-dts": "6.2.3",
    "rollup-plugin-esbuild": "6.2.1",
    "rollup-plugin-node-externals": "8.1.1",
    "rollup-plugin-peer-deps-external": "2.2.4",
    "supertest": "7.1.4",
    "typescript": "5.9.3",
    "vitest": "3.2.4"
  },
  "peerDependencies": {
    "express": "^4.0.0 || ^5.0.0"
  },
  "packageManager": "pnpm@10.18.2",
  "engines": {
    "node": ">=18"
  }
}
